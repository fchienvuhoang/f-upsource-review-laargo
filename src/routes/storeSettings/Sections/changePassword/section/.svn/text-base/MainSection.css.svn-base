@import '../../../../../components/variables.css';
@import '../../../../../components/ComponentUI/panel.css';
@import '../../../../../components/ComponentUI/button.css';
@import '../../../../../components/ComponentUI/input.css';

.wrapperNotice {
  & .noticeWrapper {
    width: 215px;
    margin: 0 auto;
    & span {
      &[data-type="success"] {
        background: #e8f7ed;
        color: #1ba84e;
        border: 1px solid #1ba84e;
        border-radius: 3px;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        padding: 5px 10px;
      }
      &[data-type="notMatch"] {
        background: #f1f1f1;
        color: #666;
        border: 1px solid #d1d0d1;
        border-radius: 3px;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        padding: 5px 10px;
      }
    }
  }
}

.wrapperChangePassword {
  width: 400px;
  & .wrapButtonAction {
    margin-top: 15px;
  }
}

.wrapDesignProductView {
  width: 98%;
  max-width: 1500px;
  & .wrapOptionDesignForm {
    margin-top: -20px;
    margin-right: 20px;
    & ul {
      float: right;
      & li {
        & a {

        }
      }
    }
  }
  & .wrapperDesignViewContent {
    min-height: 400px;
    width: 100%;
    display: inline-block;
    & .emptyBox {
      width: 500px;
      border: 1px solid var(--blue-border);
      margin: 100px auto;
      border-radius: 3px;
      padding: 20px;
      background: #F4FAFF;
      & label {
        color: var(--gray);
        font-size: 14px;
      }
    }
  }
}
